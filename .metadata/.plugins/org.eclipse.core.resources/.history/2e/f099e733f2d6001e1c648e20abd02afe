package com.dl.wrapper;

public class Eg_1 {

	public static void main(String[] args) {
		//Primitive to object type
		byte b = 10;
		Byte b1 = new Byte(b);
		System.out.println(b1);
		
		Short s = new Short(b);
		System.out.println(s);
		
		short s1 = 20;
		Byte b2 = new Byte((byte) s1);
		System.out.println(b2);

	}

}
