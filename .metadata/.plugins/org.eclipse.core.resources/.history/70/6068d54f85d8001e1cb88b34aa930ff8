package com.dl.poly;

public class Eg_1 {
	void m1(int a, int b) {
		System.out.println(a + b);
	}
	void m1(double a, double b) {
		System.out.println(a*b);
	}

	public static void main(String[] args) {
		//Polymorphism contains two methods
		//overloading and overriding
		Eg_1 poly = new Eg_1();
		poly.m1(5, 6);
		poly.m1(3, 5);

	}

}
